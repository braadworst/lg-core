{
  "name": "lr-core",
  "version": "1.0.0",
  "description": "The core for everything lagoon-road",
  "main": "index.js",
  "scripts": {
    "min": "node_modules/.bin/browserify index.js -t [ babelify --presets [ latest ] ] --ignore-missing | uglifyjs > index.min.js",
    "test": "nyc tape test/*.js | tap-spec",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "all": "npm run test && npm run report && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagoon-road/lr-core.git"
  },
  "author": "Roy Niels",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lagoon-road/lr-core/issues"
  },
  "keywords": [
    "core",
    "lagoon-road"
  ],
  "homepage": "https://github.com/lagoon-road/lr-core#readme",
  "dependencies": {
    "check-types": "^7.1.5"
  },
  "devDependencies": {
    "coveralls": "^2.13.0",
    "nyc": "^10.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "minifier": "^0.8.1",
    "uglify-js-harmony": "^2.7.5"
  }
}
